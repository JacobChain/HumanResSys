	<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
	<form action="${pageContext.request.contextPath }/train/trainFeedBack">
	<div>
	
	<table >
	<tr>
	<td>培训名称:</td>
	<td>${train.trainname}</td>
	</tr>
	<tr>
	<td>培训讲师:</td>
	<td>${train.trainteacher}</td>
	</tr>
	<tr>
	<td>开始时间:</td>
	<td>${train.starttime}</td>
	</tr>
	<tr>
	<td>结束时间:</td>
	<td>${train.endtime}</td>
	</tr>
	<tr>
	<td>培训目的:</td>
	<td>${train.trainpurpose}</td>
	</tr>
	<tr>
	<td>培训人员:</td>
	<td>${train.trainees}</td>
	</tr>
	<tr>
	<td>培训简介:</td>
	<td>${train.description}</td>
	</tr>
	
	</table>
	</div>
	<div>培训反馈</div>
	<table>
	<tr>
	<td>培训反馈:</td>
	<td>
	<select name="valueName">
	<option>--请选择--</option>
	<option>起草</option>
	<option>审核中</option>
	<option>已审核</option>
	</select></td>
	</tr>
	<tr><td>
	培训总结:
	</td>
	<td><textarea rows="10" cols="40" name="summarize"></textarea></td>
	</tr>
	<tr><td>
	考核结果:
	</td>
	<td><textarea rows="10" cols="40" name="trainResults"></textarea></td>
	</tr>
	<tr>
	<td><input type="submit" value="保存"></td>
	<td><input type="button" value="返回"></td>
	</tr>
	</table>
	</form>

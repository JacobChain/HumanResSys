<mapper namespace="cn.humanResSys.dao.job.JobMapper" >
<select id="getlist" resultType="job" parameterType="map">
	SELECT *,d.`valueName` AS department  FROM job j
LEFT JOIN dictionary d ON d.`valueId`=j.`departmentStatus`
WHERE typeCode='DEPARTMENT'
